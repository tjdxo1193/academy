package test;

public class Sum {

	public static void main(String[] args) {
		//NUM+NUM+NUM...NUM°³
		int SUM = 0;
		int NUM =8;
		for(int i = 0; i < NUM; i++) {
			SUM += NUM;
		}
		System.out.println(SUM);
		
		
		//1+2+3+4+....NUM
		SUM = 0;
		NUM = 10;
		for(int i= 1; i <= NUM ; i++) {
			SUM += i;
		}
		System.out.println(SUM);
		
		
		//1+3+5+7+....NUM
		SUM = 0;
		NUM = 10;
		for(int i= 1; i <= NUM ; i++) {
			SUM += i+2;
			//i++
		}
		
		//MUL = 1x2x3x4x5x6...NUM
		int MUL = 1;
		NUM = 10;
		for(int i= 1; i <= NUM ; i++) {
			MUL *= i;
		}
		System.out.println(MUL);
		
		//
		int[] ten =  new int[] {1, 10, 2500, 44, 140};
		NUM = 5;
		for(int i= 0; i < NUM ; i++) {
			System.out.println(Integer.toBinaryString(ten[i]));
		}
		
	}

}
