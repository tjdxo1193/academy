package thisex;

class Person{
	String name;
	int age;
	
	Person(){
		this("이름없음",1);
	}
	
	Person(String name, int age){
		this.name=name;
		this.age=age;
		
	}
	
	Person returnItSelf() {
		return this;
	}
	
}
public class CallAnotherCont {

	public static void main(String[] args) {
		Person noName=new Person();
		System.out.println(noName.name);
		
		Person p= noName.returnItSelf();
		System.out.println(p);
		
	}

}
