package chapter2;

public class BreakEx1 {

	public static void main(String[] args) {
		int num=0;
		int sum=0;
		
		for(num=0;sum<10;num++) {
			sum+=num;
	//		System.out.println(num);
		//	System.out.println(sum);
			
		}
		System.out.println("num : "+num);
		System.out.println("sum : "+sum);
	}

}
